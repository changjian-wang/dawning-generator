namespace {{namespace}}.Domain.Core.Events;

/// <summary>
/// 领域事件基类
/// </summary>
public abstract class DomainEvent
{
    /// <summary>
    /// 事件 ID
    /// </summary>
    public Guid EventId { get; } = Guid.NewGuid();
    
    /// <summary>
    /// 事件发生时间
    /// </summary>
    public DateTime OccurredOn { get; } = DateTime.UtcNow;
    
    /// <summary>
    /// 聚合根 ID
    /// </summary>
    public Guid AggregateId { get; protected set; }
}
